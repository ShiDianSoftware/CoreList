
<view class='list'>
  <corelist request_begin="{{request_begin}}" count="{{count}}" top="{{top}}" bottom="{{bottom}}" has_more="{{has_more}}" class='corelist' >
    <view class='item' wx:for="{{dataList}}">
      <view>{{index+1}}</view>
      <view>{{index+1}}: {{item.title}} {{count}}</view>
      <view>{{index+1}}: {{item.content}}</view>
    </view>
  </corelist>
</view>
